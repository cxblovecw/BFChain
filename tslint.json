{
    "rules": {
        "no-consecutive-blank-lines": true,
        "array-type": [
            true,
            "array-simple"
        ],
        "arrow-parens": true,
        "no-var-keyword": true,
        "no-unused-variable": [
            true,
            {
                "ignore-pattern": "^_"
            }
        ],
        "ordered-imports": false,
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "indent": [
            true,
            "spaces"
        ],
        "no-eval": true,
        "no-internal-module": true,
        "no-trailing-whitespace": false,
        "no-unsafe-finally": true,
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ],
        "quotemark": false,
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "whitespace": false
    },
    "jsRules": {
        "triple-equals": [
            true,
            "allow-null-check"
        ]
    },
    "defaultSeverity": "warning"
}